            <TextView
                android:id="@+id/tvProfileName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Dummy Name"
                android:layout_alignBaseline="@+id/textView5"
                android:layout_alignBottom="@+id/textView5"
                android:layout_toRightOf="@+id/textView5"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginLeft="30dp"
                android:lines="1"
                android:ellipsize="none"
                android:layout_marginRight="10dp" />

            <TextView
                android:text="Alamat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/tvProfileName"
                android:id="@+id/textView7"
                android:layout_marginTop="3dp" />

            <TextView
                android:text="Dummy Jabatan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView7"
                android:layout_alignLeft="@+id/tvProfileName"
                android:layout_alignStart="@+id/tvProfileName"
                android:id="@+id/tvJabatan"
                android:lines="1"
                android:ellipsize="none"
                android:layout_alignRight="@+id/tvProfileName"
                android:layout_alignEnd="@+id/tvProfileName" />

            <TextView
                android:text="Nama Propinsi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/tvJabatan"
                android:id="@+id/textView11"
                android:layout_marginTop="3dp" />

            <TextView
                android:text="Dummy Region"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView11"
                android:layout_alignLeft="@+id/tvJabatan"
                android:layout_alignStart="@+id/tvJabatan"
                android:layout_alignRight="@+id/tvJabatan"
                android:layout_alignEnd="@+id/tvJabatan"
                android:lines="1"
                android:ellipsize="none"
                android:id="@+id/tvRegion" />

            <TextView
                android:text="Kab Kota"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/tvRegion"
                android:id="@+id/textView16"
                android:layout_marginTop="3dp" />

            <TextView
                android:text="Dummy Cabang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView16"
                android:layout_alignLeft="@+id/tvRegion"
                android:layout_alignStart="@+id/tvRegion"
                android:id="@+id/tvCabang"
                android:layout_alignRight="@+id/tvRegion"
                android:layout_alignEnd="@+id/tvRegion"
                android:lines="1"
                android:ellipsize="none"/>

            <TextView
                android:text="Date Insert"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:id="@+id/textView18"
                android:layout_below="@+id/tvCabang"
                android:layout_marginTop="3dp" />

            <TextView
                android:text="Dummy Points"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView18"
                android:layout_alignBottom="@+id/textView18"
                android:layout_alignLeft="@+id/tvCabang"
                android:layout_alignStart="@+id/tvCabang"
                android:layout_alignRight="@+id/tvCabang"
                android:layout_alignEnd="@+id/tvCabang"
                android:id="@+id/tvPoints"
                android:lines="1"
                android:ellipsize="none"/>

            <TextView
                android:text="Status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:id="@+id/textView19"
                android:layout_below="@+id/tvPoints"
                android:layout_marginTop="3dp" />

            <TextView
                android:text="Dummy Status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView19"
                android:layout_alignBottom="@+id/textView19"
                android:layout_alignLeft="@+id/tvPoints"
                android:layout_alignStart="@+id/tvPoints"
                android:layout_alignRight="@+id/tvPoints"
                android:layout_alignEnd="@+id/tvPoints"
                android:id="@+id/tvStatus"
                android:lines="1"
                android:ellipsize="none"/>

