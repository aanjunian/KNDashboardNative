                <!--android:layout_marginTop="111dp"-->
                <!--android:id="@+id/tvRanking"-->
                <!--android:layout_alignRight="@+id/tvPoints"-->
                <!--android:layout_alignEnd="@+id/tvPoints"-->
                <!--android:layout_marginBottom="10dp"/>-->

        </RelativeLayout>

    </android.support.v7.widget.CardView>

</RelativeLayout>