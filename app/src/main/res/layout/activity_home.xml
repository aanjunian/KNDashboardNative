        <TableLayout
            android:id="@+id/tl_datahome"
            android:padding="10dp"
            android:layout_below="@+id/slider"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >
        </TableLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tl_datahome"
            android:padding="10dp">

            <TextView
                android:id="@+id/tv_role"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Role"/>

            <Spinner
                android:id="@+id/spn_role"
                android:layout_below="@+id/tv_role"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_edtext" />

            <TextView
                android:id="@+id/tv_branch"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/spn_role"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Branch"/>

            <Spinner
                android:id="@+id/spn_branch"
                android:layout_below="@+id/tv_branch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_edtext" />

            <Button
                android:text="Process"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/spn_branch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/btn_process"
                android:textColor="#FFF"
                android:background="@drawable/btn_innermenu"/>

        </RelativeLayout>



